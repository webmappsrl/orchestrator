{
    "name": "wm/kanban-card",
    "description": "A reusable and parameterized Kanban board card for Laravel Nova 4.",
    "keywords": [
        "laravel",
        "nova",
        "kanban"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Webmapp\\KanbanCard\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webmapp\\KanbanCard\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
